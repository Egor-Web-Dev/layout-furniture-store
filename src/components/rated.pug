section.rated.container
  h2.rated__title Высокий рейтинг
  ul.list-reset.rated__products
    each data in [{ img: "product-1", name: "Диван кожаный ", model: "“R-94”", price: "94 990 руб", rating: "5,0" }, { img: "product-2", name: "Диван апартековый ", model: "“T-75”", price: "69 990 руб", rating: "4,9" }, { img: "product-3", name: "Диван тканевый ", model: "“D-31”", price: "28 490 руб", rating: "4,8" }, { img: "product-4", name: "Диван велюровый ", model: "“Y-68”", price: "22 990 руб", rating: "4,8" }, { img: "product-5", name: "Диван из шинила ", model: "“W-95”", price: "22 990 руб", rating: "4,8" }, { img: "product-6", name: "Диван флоковый ", model: "“G-41”", price: "17 990 руб", rating: "4,8" }, { img: "product-7", name: "Диван шиниловый ", model: "“V-43”", price: "19 990 руб", rating: "4,8" }, { img: "product-8", name: "Диван велюровый ", model: "“S-99”", price: "19 990 руб", rating: "4,7" },]
      li.rated__product
        - var className = "rated__card"
        - var productImg = data.img
        - var productName = data.name
        - var productModel = data.model
        - var productPrice = data.price
        - var productRating = data.rating
        include ./product-card
